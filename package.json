    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@uniswap/lib": "1.1.1",
    "@uniswap/v2-core": "^1.0.1",
    "abi-decoder": "^2.4.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.4",
    "child_process": "^1.0.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "delay": "^4.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "hardhat": "^2.16.0",
    "hardhat-contract-sizer": "^2.3.0",
    "hardhat-deploy": "^0.9.24",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.4",
    "keccak": "^3.0.0",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "pinst": "^2.1.6",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "querystring": "^0.2.0",
    "rlp": "^2.2.5",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "truffle-flattener": "1.5.0",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.5",
    "typescript": "^4.5.2",
    "web3-eth-abi": "^1.6.1"
  },
  "files": [
    "/contracts"
  ],
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paraswap/paraswap-contracts.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paraswap/paraswap-contracts/issues"
  },
  "homepage": "https://gitlab.com/paraswap/paraswap-contracts#readme",
  "scripts": {
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./src/types ./coverage.json",
    "commit": "git-cz",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile --max-memory 4096",
    "coverage": "yarn typechain && hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "test:routers": "hardhat test ./test/routers/*.spec.ts --deploy-fixture",
    "test:fee": "hardhat test ./test/fee/*.spec.ts --deploy-fixture",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "deploy:rinkeby": "yarn compile && yarn hardhat --network rinkeby deploy",
    "deploy:ropsten": "yarn compile && yarn hardhat --network ropsten deploy",
    "deploy:mainnet": "yarn compile && yarn hardhat --network mainnet deploy",
    "deploy:polygon": "yarn compile && yarn hardhat --network polygon deploy",
    "deploy:polygonZkEvm": "yarn compile && yarn hardhat --network polygonZkEvm deploy",
    "deploy:bsc": "yarn compile && yarn hardhat --network bsc deploy",
    "deploy:avalanche": "yarn compile && yarn hardhat --network avalanche deploy",
    "deploy:fantom": "yarn compile && yarn hardhat --network fantom deploy",
    "deploy:arbitrum": "yarn compile && yarn hardhat --network arbitrum deploy",
    "deploy:optimism": "yarn compile && yarn hardhat --network optimism deploy",
    "deploy:base": "yarn compile && yarn hardhat --network base deploy",
    "deploy:tenderly": "yarn compile && yarn hardhat --network tenderly deploy"
  },
  "dependencies": {
    "@openzeppelin/contracts": "v3.4.2",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-periphery": "^1.3.0"
  },
  "optionalDependencies": {
    "scrypt": "^6.0.3"
  }
}
